                            <!-- end menu sublinea producto -->
                        </ul>
                    </div>
                </div>
            </div>
            <!-- end sidebar menu -->
            <!-- start page content -->
            <div class="page-content-wrapper">
                <div class="page-content">

                    @RenderBody()
                </div>
            </div>
            <!-- end page content -->
            <!-- start chat sidebar -->
            <div class="chat-sidebar-container" data-close-on-body-click="false">
                <div class="chat-sidebar">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a href="#quick_sidebar_tab_1" class="nav-link active tab-icon" data-toggle="tab">
                                Theme
                            </a>
                        </li>

                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane chat-sidebar-settings in show active animated shake" role="tabpanel" id="quick_sidebar_tab_1">
                            <div class="slimscroll-style">
                                <div class="theme-light-dark hidden">
                                    <h6>Sidebar Theme</h6>
                                    <button type="button" data-theme="white" class="btn lightColor btn-outline btn-circle m-b-10 theme-button">Light Sidebar</button>
                                    <button type="button" data-theme="dark" class="btn dark btn-outline btn-circle m-b-10 theme-button">Dark Sidebar</button>
                                </div>
                                <div class="theme-light-dark">
                                    <h6>Sidebar Color</h6>
                                    <ul class="list-unstyled">
                                        <li class="complete">
                                            <div class="theme-color sidebar-theme">
                                                <a href="#" onclick="configurarSideBar('white')" data-theme="white"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarSideBar('dark')" data-theme="dark"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarSideBar('blue')" data-theme="blue"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarSideBar('indigo')" data-theme="indigo"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarSideBar('cyan')" data-theme="cyan"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarSideBar('green')" data-theme="green"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarSideBar('red')" data-theme="red"><span class="head"></span><span class="cont"></span></a>
                                            </div>
                                        </li>
                                    </ul>
                                    <h6>Header Brand color</h6>
                                    <ul class="list-unstyled">
                                        <li class="theme-option">
                                            <div class="theme-color logo-theme">
                                                <a href="#" onclick="configurarLogo('white')" data-theme="logo-white"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarLogo('dark')" data-theme="logo-dark"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarLogo('blue')" data-theme="logo-blue"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarLogo('indigo')" data-theme="logo-indigo"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarLogo('cyan')" data-theme="logo-cyan"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarLogo('green')" data-theme="logo-green"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarLogo('red')" data-theme="logo-red"><span class="head"></span><span class="cont"></span></a>
                                            </div>
                                        </li>
                                    </ul>
                                    <h6>Header color</h6>
                                    <ul class="list-unstyled">
                                        <li class="theme-option">
                                            <div class="theme-color header-theme">
                                                <a href="#" onclick="configurarHeader('white')" data-theme="header-white"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarHeader('dark')" data-theme="header-dark"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarHeader('blue')" data-theme="header-blue"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarHeader('indigo')" data-theme="header-indigo"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarHeader('cyan')" data-theme="header-cyan"><span class="head"></span><span class="cont"></span></a>
                                                <a href="#" onclick="configurarHeader('green')" data-theme="header-green"><span class="head"></span><span class="cont"></span></a>
