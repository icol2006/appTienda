    @Scripts.Render("~/bundles/jquery")

    @Scripts.Render("~/bundles/bootstrap")


    @RenderSection("scripts", required: false)

    <script>
        function configurarSideBar(datos)
        {
            $.ajax({
            cache: false,
            type: "GET",
                url: '@Url.Action("SetSideBar", "Theme")',
                datatype: "json",
                data: { "color": datos},
                success: function (data) {
                    setSideBar(datos);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
            });
        }
        function configurarHeader(datos) {
                    $.ajax({
            cache: false,
            type: "GET",
                url: '@Url.Action("SetHeader", "Theme")',
                datatype: "json",
                data: { "color": datos},
                success: function (data) {
                    SetHeader(datos);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
            });
        }
        function configurarLogo(datos) {
                    $.ajax({
            cache: false,
            type: "GET",
                url: '@Url.Action("SetLogo", "Theme")',
                datatype: "json",
                data: { "color": datos},
                success: function (data) {
                    SetLogo(datos);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
            });
        }



            //Cargar themes
            $.ajax({
            cache: false,
            type: "GET",
                url: '@Url.Action("LoadTheme", "Theme")',
                datatype: "json",
                success: function (data) {

            },
                error: function (xhr, ajaxOptions, thrownError) {
            }
            });


        //Obtener theme actual
            $.ajax({
            cache: false,
            type: "GET",
                url: '@Url.Action("GetSideBar", "Theme")',
                datatype: "json",
                success: function (data) {
                    setSideBar(data);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
            });


            //get current Header
            $.ajax({
            cache: false,
            type: "GET",
                url: '@Url.Action("GetHeader", "Theme")',
                datatype: "json",
                success: function (data) {
                    setHeader(data);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
            });


            //get current Logo
            $.ajax({
            cache: false,
            type: "GET",
                url: '@Url.Action("GetLogo", "Theme")',
                datatype: "json",
                success: function (data) {
                    setLogo(data);
            },
            error: function (xhr, ajaxOptions, thrownError) {
                alert('Failed to retrieve books.');
            }
        });


        //SideBar Configuracion
        function setSideBar(datos)
        {
            var color = datos;
            jQuery("body").removeClass("white-sidebar-color dark-sidebar-color blue-sidebar-color indigo-sidebar-color green-sidebar-color red-sidebar-color cyan-sidebar-color");
            switch (color) {
                case "white":
                    jQuery("body").addClass("white-sidebar-color");
                    break;
                case "dark":
                    jQuery("body").addClass("dark-sidebar-color");
                    break;
                case "blue":
                    jQuery("body").addClass("blue-sidebar-color");
                    break;
                case "indigo":
                    jQuery("body").addClass("indigo-sidebar-color");
                    break;
                case "green":
                    jQuery("body").addClass("green-sidebar-color");
                    break;
                case "red":
                    jQuery("body").addClass("red-sidebar-color");
                    break;
                case "cyan":
                    jQuery("body").addClass("cyan-sidebar-color");
                    break;
                case "default":
                    jQuery("body").addClass("dark-sidebar-color");
                    break;
            }
        }



        //Configuracion logo
        function setLogo(datos)
        {
            var color = datos;
            jQuery("body").removeClass("logo-white logo-dark logo-blue logo-indigo logo-red logo-cyan logo-green");

            switch (color) {
                case "white":
                    jQuery("body").addClass("logo-white");
                    break;
                case "dark":
                    jQuery("body").addClass("logo-dark");
                    break;
                case "blue":
                    jQuery("body").addClass("logo-blue");
                    break;
                case "indigo":
                    jQuery("body").addClass("logo-indigo");
                    break;
                case "green":
                    jQuery("body").addClass("logo-green");
                    break;
                case "red":
                    jQuery("body").addClass("logo-red");
                    break;
                case "cyan":
                    jQuery("body").addClass("logo-cyan");
                    break;
                default:
                    jQuery("body").addClass("indigo-white");
                    break;
            }
        }

        //Configuracion Header
        function setHeader(datos)
        {
            var color = datos;
            jQuery("body").removeClass("header-white header-dark header-blue header-indigo header-red header-cyan header-green");

            switch (color) {
                case "white":
                    jQuery("body").addClass("header-white");
                    break;
                case "dark":
                    jQuery("body").addClass("header-dark");
                    break;
                case "blue":
                    jQuery("body").addClass("header-blue");
                    break;
                case "indigo":
                    jQuery("body").addClass("header-indigo");
                    break;
                case "green":
                    jQuery("body").addClass("header-green");
                    break;
                case "red":
                    jQuery("body").addClass("header-red");
                    break;
                case "cyan":
                    jQuery("body").addClass("header-cyan");
                    break;
                default:
                    jQuery("body").addClass("header-blue");
                    break;
            }
        }


    </script>

    @if (@ViewBag.resultado != null)
    {
        <script>
        alert('@ViewBag.resultado');
        </script>
    }


</body>
</html>
